<dataset>
  <module>config</module>
  <code>cfg_mgr_program_DS4Q</code>
  <entity>cfg_mgr_program</entity>
  <name>项目【查询】</name>
  <fields>
    <field code="id" name="项目id" edit-type="hidden" is-key="true" not-null="true">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="code_LKE" name="项目编码" edit-type="input" not-null="true"/>
    <field code="name_LKE" name="项目名称" edit-type="input" not-null="true"/>
    <field code="description_LKE" name="项目描述" edit-type="input"/>
    <field code="status" name="状态" edit-type="select">
      <enum-class code="cfg_mgr_program.status"/>
    </field>
    <field code="creator_id" name="创建人" edit-type="hidden">
      <rel entity-code="user/user_id/user_name" add-by-global="true"/>
    </field>
    <field code="create_time" name="创建时间" edit-type="hidden"/>
    <field code="modifier_id" name="修改人" edit-type="hidden">
      <rel entity-code="user/user_id/user_name" add-by-global="true"/>
    </field>
    <field code="modify_time" name="修改时间" edit-type="hidden"/>
  </fields>
</dataset>
