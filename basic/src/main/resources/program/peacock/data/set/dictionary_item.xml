<dataset>
  <module>system</module>
  <code>dictionary_item</code>
  <entity>dictionary_item</entity>
  <name>字典项【默认】</name>
  <fields>
    <field code="dictionary_item_id" name="字典项ID" edit-type="input" is-key="true">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="value" name="字典项值" edit-type="input"/>
    <field code="text" name="字典项文本" edit-type="input"/>
    <field code="desc" name="字典项描述" edit-type="input"/>
    <field code="is_default" name="是否默认" edit-type="input"/>
    <field code="pri" name="优先级" edit-type="input" show-type="hidden">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="ext1" name="扩展字段1" edit-type="input"/>
    <field code="ext2" name="扩展字段2" edit-type="input"/>
    <field code="dictionary_id" name="字典ID" edit-type="select">
      <rel entity-code="dictionary/dictionary_id/dictionary_name"/>
    </field>
    <field code="dictionary_code" name="" edit-type="input"/>
    <field code="op_id" name="创建人" edit-type="input">
      <rel entity-code="user/user_id/user_name" add-by-global="true"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="create_time" name="创建时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="modify_op_id" name="修改人" edit-type="input">
      <rel entity-code="user/user_id/user_name" add-by-global="true"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="modify_time" name="修改时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="del_flag" name="删除标识" edit-type="input" not-null="true">
      <create-edit-type>hidden</create-edit-type>
    </field>
  </fields>
</dataset>
