<dataset>
  <module>config</module>
  <code>cfg_runtime_response_DS4Q</code>
  <entity>cfg_runtime_response</entity>
  <name>响应参数【查询】</name>
  <fields>
    <field code="id" name="参数id" edit-type="hidden" is-key="true" not-null="true"/>
    <field code="code_LKE" name="参数编码" edit-type="input" not-null="true"/>
    <field code="program_id" name="项目id" edit-type="select" not-null="true">
      <rel entity-code="cfg_mgr_program/id/name"/>
    </field>
    <field code="name_LKE" name="参数名称" edit-type="input" not-null="true"/>
    <field code="value_LKE" name="参数取值" edit-type="input"/>
    <field code="type" name="参数类型" edit-type="input"/>
    <field code="status" name="状态" edit-type="input"/>
    <field code="creator_id" name="创建人" edit-type="hidden">
      <rel entity-code="user/user_id/user_name" add-by-global="true"/>
    </field>
    <field code="create_time" name="创建时间" edit-type="hidden"/>
    <field code="modifier_id" name="修改人" edit-type="hidden">
      <rel entity-code="user/user_id/user_name" add-by-global="true"/>
    </field>
    <field code="modify_time" name="修改时间" edit-type="hidden"/>
    <field code="description_LKE" name="参数描述" edit-type="input"/>
  </fields>
</dataset>
